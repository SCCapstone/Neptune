<resources>
    <string name="app_name">Neptune Client</string>


    <string name="instructionsExplanation">The following shows a list of instructions to follow to enable the reading of notifications.</string>
    <string name="notification_permission_step1">1. Open the Settings App</string>
    <string name="notification_permission_step2">2. Search for "special app access"</string>
    <string name="notification_permission_step3">3. Click "device &amp; app notifications"</string>
    <string name="notification_permission_step4">4. Click &quot;Neptune Client&quot;</string>
    <string name="notification_permission_step5">5. Enable the &quot;Allow notification access&quot; switch.</string>
    <string name="notification_permission_step6">6. After enabling the setting, restart Neptune Client.</string>


    <string name="notification_permission_title">Notification Access Required</string>
    <string name="notification_permission_description">This app requires access to your notifications in order to function properly. To grant access, please enable the notification access permission for this app in the settings.</string>
    <string name="open_settings">Open Settings</string>
</resources>